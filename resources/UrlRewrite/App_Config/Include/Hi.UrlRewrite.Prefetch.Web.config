<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/"
               xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone or ContentDelivery">
    <databases>
      <database id="web">
        <dataProviders hint="list:AddDataProvider">
          <dataProvider type="Sitecore.Data.DataProviders.CompositeDataProvider, Sitecore.Kernel">
            <param desc="headProvider">
              <dataProvider ref="dataProviders/main" param1="$(id)">
                <prefetch hint="raw:AddPrefetch">
                  <!--url rewrite prefetch -->
                  <item desc="urlrewriter">{3CF68609-B1F2-4ADE-B7E3-91B5CF74F5B8}</item>
                </prefetch>
              </dataProvider>
            </param>
          </dataProvider>
        </dataProviders>
      </database>
    </databases>
  </sitecore>
</configuration>